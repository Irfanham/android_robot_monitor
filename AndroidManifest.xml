<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="org.ros.android.robot_monitor" android:versionCode="1" android:versionName="0.1">
	<uses-sdk android:minSdkVersion="9" />
